{
  "name": "@neopass/wordlist",
  "version": "0.3.4",
  "description": "Create a word list from a system dictionary",
  "main": "lib/index.js",
  "scripts": {
    "build": "./word-gen && tsc && babel lib --out-dir lib",
    "clean": "rm -rf lib",
    "watch": "tsc --watch",
    "prepublishOnly": "npm run clean && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/neopass/wordlist.git"
  },
  "keywords": [
    "words",
    "word list",
    "wordlist",
    "english words",
    "generator",
    "scowl"
  ],
  "author": "James Abney",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/neopass/wordlist/issues"
  },
  "homepage": "https://github.com/neopass/wordlist#readme",
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/plugin-transform-runtime": "^7.4.4",
    "@babel/preset-env": "^7.4.5",
    "@types/node": "^12.0.2",
    "@types/yargs": "^13.0.0",
    "typescript": "^3.5.1"
  },
  "dependencies": {
    "@babel/runtime-corejs2": "^7.4.5",
    "yargs": "^13.2.4"
  },
  "bin": {
    "wordlist-gen": "bin/word-gen"
  },
  "files": [
    "lib",
    "utils",
    "bin",
    "default.txt"
  ]
}
